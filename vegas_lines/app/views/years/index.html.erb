<h1> Year Index </h1>
<hr />

<div id="years">
  <table class="table_list">
    <tr>
      <th>Season</th>
    </tr>
  <% if @years != nil %>
    <% @years.each do |year| %>
      <tr class="clickable-row" onclick="location.href='<%= year_weeks_path(year.id) %>'">
	<td><%= link_to "#{year.year}", year_weeks_path(year.id)  %></td>
      </tr>
    <% end %>
  <% end %>
  </table>
</div>
<div class="links">
  <%= link_to "User Index", user_path(current_user) %>
  <% if current_user.admin? %>
    | <%= link_to 'New Season', new_year_path %>
  <% end %>
</div>